    /*API javascript boite meteo panoramique*/

    const APIKEY = '6c768a12332784de4954dddb19d855d9';

    let url = `https://api.openweathermap.org/data/2.5/weather?q=Cilaos&appid=${APIKEY}&units=metric&lang=fr`;

    fetch(url)
     .then( (response) =>
      response.json().then((data) => {
        console.log(data);
        document.querySelector('#city').innerHTML = data.name;
        document.querySelector('#temp').innerHTML = "<i class='fas fa-thermometer-quarter'></i>" + data.main.temp + '°';
